#include <stdio.h>

int main() {
    int arr[5] = {10, 25, 5, 40, 15};
    int largest = arr[0];

    // Find the largest element
    for (int i = 1; i < 5; i++) {
        if (arr[i] > largest) {
            largest = arr[i];
        }
    }

    printf("The largest element is: %d\n", largest);

    return 0;
}
