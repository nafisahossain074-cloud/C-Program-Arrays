#include <stdio.h>

int main() {
    int arr[5] = {50, 20, 40, 10, 30};
    int i, j, key;
    for (i = 1; i < 5; i++) {
        key = arr[i];
        j = i - 1;
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = key;
    }
    printf("Array after Insertion Sort:\n");
    for (i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
