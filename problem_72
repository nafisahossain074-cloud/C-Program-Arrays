#include <stdio.h>

int main() {
    int arr[10] = {10, 20, 20, 30, 30, 30, 40, 50, 50, 60};
    int n = 10;
    int temp[10];
    int j = 0;
    temp[j++] = arr[0];
    for (int i = 1; i < n; i++) {
        if (arr[i] != arr[i - 1]) {
            temp[j++] = arr[i];
        }
    }
    printf("Array after removing duplicates:\n");
    for (int i = 0; i < j; i++) {
        printf("%d ", temp[i]);
    }
    printf("\n");

    return 0;
}
