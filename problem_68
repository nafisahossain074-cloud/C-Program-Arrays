#include <stdio.h>

int main() {
    int arr[5] = {50, 20, 40, 10, 30};
    int i, j, minIndex, temp;
    for (i = 0; i < 5 - 1; i++) {
        minIndex = i;
        for (j = i + 1; j < 5; j++) {
            if (arr[j] < arr[minIndex]) {
                minIndex = j;
            }
        }
        temp = arr[i];
        arr[i] = arr[minIndex];
        arr[minIndex] = temp;
    }
    printf("Array after Selection Sort:\n");
    for (i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
