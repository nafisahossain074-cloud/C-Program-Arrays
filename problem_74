#include <stdio.h>

int main() {
    int arr[10] = {10, 20, 30, 20, 40, 20, 50};
    int n = 7;
    int element = 20;
    int i, j = 0;
    int newArr[10];

    for (i = 0; i < n; i++) {
        if (arr[i] != element) {
            newArr[j++] = arr[i];
        }
    }
    printf("Array after removing %d:\n", element);
    for (i = 0; i < j; i++) {
        printf("%d ", newArr[i]);
    }
    printf("\n");

    return 0;
}
