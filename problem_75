#include <stdio.h>

int main() {
    int arr1[5] = {10, 20, 30, 40, 50};
    int arr2[5] = {20, 40, 60, 80, 100};
    int i, j;

    printf("Common elements in the arrays are:\n");

    for (i = 0; i < 5; i++) {
        for (j = 0; j < 5; j++) {
            if (arr1[i] == arr2[j]) {
                printf("%d ", arr1[i]);
                break;
            }
        }
    }
    printf("\n");

    return 0;
}
